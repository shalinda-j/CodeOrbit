# AI API Keys - Rename this file to .env and fill in your keys

# OpenAI
OPENAI_API_KEY=sk-...

# Google Gemini
GEMINI_API_KEY=your_gemini_api_key

# Anthropic Claude
CLAUDE_API_KEY=your_claude_api_key

# DeepSeek
DEEPSEEK_API_KEY=your_deepseek_api_key

# Alibaba Qwen
QWEN_API_KEY=your_qwen_api_key

# Optional: Set default model (openai, gemini, claude, deepseek, qwen)
DEFAULT_MODEL=openai

# Optional: Set request timeout in seconds
REQUEST_TIMEOUT=30
